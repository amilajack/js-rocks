{
  "hosting": [
    {
      "target": "js-rocks",
      "public": "dist/js-rocks",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**",
        "3rdpartylicenses.txt"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ],
      "headers": [
        {
          "source": "**/*.@(jpg|jpeg|gif|png|webp|svg|woff|woff2)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=604800"
            }
          ]
        },
        {
          "source": "**/*.@(wav|ogg)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=604800"
            }
          ]
        }
      ]
    }
  ]
}
